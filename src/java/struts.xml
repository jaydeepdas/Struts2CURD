<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="registeruser" class="struts2.test.actions.RegisterAction">
            <result name="REGISTER">/register.jsp</result>
        </action>
        <action name="report" class="struts2.test.actions.ReportAction">
            <result name="REPORT">/report.jsp</result>
        </action>
        <action name="updatedetails" class="struts2.test.actions.UpdateAction">
            <result name="UPDATE">/update.jsp</result>
        </action>
        <action name="deleterecord" class="struts2.test.actions.DeleteAction">
            <result name="DELETE">/delete.jsp</result>
        </action>
        <action name="saveproduct" class="struts2.test.actions.ProductAction" method="saveProduct">
            <result name="SAVEPRODUCT">/product.jsp</result>
        </action>
        <action name="showproduct" class="struts2.test.actions.ProductAction" method="showProduct">
            <result name="SHOWPRODUCT">/productlist.jsp</result>
        </action>
    </package>
</struts>
